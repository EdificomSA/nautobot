{
    "compilerOptions": {
        "strict": true,
        "allowJs": true,
        "baseUrl": ".",
        "paths": {
            "@nautobot/views/*": [
                "src/views/*"
            ],
            "@nautobot/components/*": [
                "src/components/*"
            ],
            "@nautobot/layouts/*": [
                "src/components/layouts/*"
            ],
            "@nautobot/common/*": [
                "src/components/common/*"
            ],
            "@nautobot/utils/*": [
                "src/utils/*"
            ],
            "@nautobot/core/*": [
                "src/components/core/*"
            ],
            "@nautobot/*": [
                "src/*"
            ]
        }
    }
}