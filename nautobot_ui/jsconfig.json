{
    "compilerOptions": {
        "strict": true,
        "allowJs": true,
        "baseUrl": ".",
        "paths": {
            "@nautobot/*": ["src/*"],
            "@nautobot/views": ["src/views/*"],
            "@utils/*": [
                "src/utils/*"
            ],
            "@views/*": [
                "src/views/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@layouts/*": [
                "src/components/layouts/*"
            ],
            "@common/*": [
                "src/components/common/*"
            ],
            "@core/*": [
                "src/components/core/*"
            ]
        }
    }
}